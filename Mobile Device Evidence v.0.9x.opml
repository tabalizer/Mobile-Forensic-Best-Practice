				<outline text="Evidence Preservation (1st Responder)">
					<outline text="Device On?">
						<outline text="YES">
							<outline text="Can You Access System Settings">
								<outline text="YES">
									<outline text="Enable Airplane Mode, Disable Wifi/BT"></outline>
									<outline text="Turn On USB Debugging &amp; Stay Awake (Android)"></outline>
									<outline text="Extend Display Auto Lock/ Timeout &amp; Lock Timer"></outline>
									<outline text="Do Not Turn Off - Keep Charged"></outline>
									<outline text="Collect Device Identifier"></outline>
								</outline>
								<outline text="NO">
									<outline text="Do Not Turn Off - Keep Charged"></outline>
									<outline text="Collect Device Identifier"></outline>
									<outline text="Put In Faraday Bag"></outline>
								</outline>
							</outline>
						</outline>
						<outline text="NO">
							<outline text="DO NOT TURN DEVICE ON">
								<outline text="Was Device Discovered In Liquid">
									<outline text="YES">
										<outline text="Keep Device In Liquid Until Cleaned"></outline>
										<outline text="Collect Device Identifier If Possible (ANDROID)"></outline>
										<outline text="Remove Battery if Possible"></outline>
									</outline>
									<outline text="NO">
										<outline text="Remove Battery If possible"></outline>
										<outline text="Collect Device Identifier"></outline>
									</outline>
								</outline>
							</outline>
						</outline>
					</outline>
				</outline>
			</outline>
